<script language="javascript">
function check(form) {
if (form.userid.value === "msLaing" && form.pswrd.value === "haha") {
   window.open('https://kushhhhh.github.io/details-page.html')
}
else {
   println("Error! Password and Username do not match... try again!")
}
}
</script>
